{
  "name": "react-input-valid",
  "version": "1.0.0",
  "description": "Smarter INPUT with validations. ONLY depends on ReactJS. Make life simple.",
  "main": "dist/react-input-valid.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --content-base=demo",
    "build": "NODE_ENV=production babel-node ./scripts/build",
    "build.demo": "WEBPACK=demo webpack",
    "test": "nyc ava -v",
    "test.report": "nyc report --reporter=html && open coverage/index.html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-core/register",
      "./test/setup"
    ],
    "files": [
      "test/*.spec.js"
    ]
  },
  "nyc": {},
  "keywords": [
    "react",
    "validate",
    "validation",
    "input",
    "form"
  ],
  "author": "aqutw",
  "license": "",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chalk": "^1.1.3",
    "coveralls": "^2.11.9",
    "del": "^2.2.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.11.1",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.3",
    "jsdom": "^9.2.1",
    "nyc": "^6.4.4",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "material-ui": "^0.15.0",
    "react": "^15.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aqutw/react-input-valid.git"
  },
  "bugs": {
    "url": "https://github.com/aqutw/react-input-valid/issues"
  },
  "homepage": "https://github.com/aqutw/react-input-valid#readme"
}
